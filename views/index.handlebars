<div id="container">
  <div id="header"><h1 class="pad-text">{{title}}</h1></div>
  <div id="hidden-preload-image" class="hidden"><img src="{{img}}"></div>
  <div id="comic" style="background-image: url('{{img}}');">&nbsp;</div>
  <div id="footer">
    <div class="align-bottom pad-text">
      <p>{{alt}}</p>
      <p><a href="{{url}}">xkcd #{{num}}</a></p>
    </div>
  </div>
</div>
